{
    order plausible before file_server
    order plausible before reverse_proxy
}

http://localhost:8080 {
    root /tmp/html

    @foo path /foo

    route {
        plausible {
            domain_name muetsch.io
            base_url https://plausible.io
        }
        respond @foo "Foo" 200
        file_server
    }
}